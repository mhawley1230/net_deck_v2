<h1><%= @tournament.name %></h1>
<h2><%= @tournament.location %></h2>
No. of Players: <h2><%= @tournament.no_of_players %></h2>

<% @decks.each do |deck| %>
  <div class='deck'>
    <%= link_to deck.name, deck_path(deck) %>
  </div>
<% end %>

<div id='deck-content'></div>
<div id='card-img'></div>
